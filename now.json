{
  "name": "platzi-badges",
  "version": 2,
  "builds": [{ "src": "./index.js", "use": "@now/node-server" }],
  "routes": [{ "src": "/.*", "dest": "/index.js" }],
  "env": {
    "PORT": "@pb-port",
    "DB_USER": "@pb-db-user",
    "DB_PASSWORD": "@pb-db-password",
    "DB_HOST": "@pb-db-host",
    "DB_NAME": "@pb-db-name"
  }
}
